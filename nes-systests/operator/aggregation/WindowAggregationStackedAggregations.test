# 1: Two window aggregations with a keyed sliding and then a keyed tumbling window
SELECT start, end, id, SUM(inputValue) as sumValue FROM (
    SELECT SUM(value) as inputValue FROM stream WHERE id < 15 GROUP BY id WINDOW TUMBLING(timestamp, size 2 sec)
) WHERE id < 10 GROUP BY id WINDOW SLIDING(start, size 1 sec, advance by 500 ms) INTO sinkStream;
