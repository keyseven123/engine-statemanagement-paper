# The global variables file

# The build-dir to use for copying binaries
# This should be the suffix of your CLion mappings
cmake_build_dir: nebulastream/cmake-build-fat-1-debug/nes-core

# The worker root dir to use for deployments
worker_root_dir: /local-ssd

# The coordinator root dir to use for deployments
coord_root_dir: /data-ssd

# Worker binary name
worker_bin_name: nesWorker

# Coordinator binary name
coord_bin_name: nesCoordinator

# Worker conf file
worker_conf_name: worker.yaml

# Coordinator conf file
coord_conf_file: coordinator.yaml